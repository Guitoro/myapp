<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header_ordenadores.ejs %>
<style>
table, th, td {
  border: 2px solid black;
  border-collapse: collapse;
}
th {
  text-align: left
}
td {
  text-align: left
}

</style>
</head>

<body>



<div class="container">
<h2>Ordenadores Disponibles</h2>

<table style = "width:50%">

<tr>
  <th>Marca</th>
  <th>Estado</th>
  <th>Precios</th>
</tr>

    <% results.forEach(function(r) { %> <tr>
        <td><%= r.marca %></td> 
	<td><%= r.estado %> </td>
 	<td><%= r.precio %> &#8364 </td>
    <% }); %> </tr>

</table>

</div>

</body>
</html>
